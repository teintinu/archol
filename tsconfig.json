{
  "compilerOptions": {
    "incremental": true,
    "target": "ES2017",
    "module": "commonjs",
    "lib": [
      "ES2017"
    ],
    "sourceMap": true,
    "outDir": ".temp/dist",
//    "tsBuildInfoFile": ".temp/cache",
    "rootDir": "./",
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": [
    "ide/**/*.ts",
    "builder/**/*.ts",
  ],
  "exclude": [
    "sampleOut/**/*.*"
  ]
}