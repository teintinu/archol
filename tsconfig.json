{
  "compilerOptions": {
    "incremental": true,
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "ES2020"
    ],
    "sourceMap": true,
    "outDir": ".temp/dist",
    "tsBuildInfoFile": ".temp/cache",
    "rootDir": "./",
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  }
}